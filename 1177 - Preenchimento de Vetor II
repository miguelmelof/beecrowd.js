var input = require('fs').readFileSync('/dev/stdin', 'utf8');
var lines = input.split('\n');
var prompt = function(texto) { return lines.shift();};
var N = []
var a = parseInt(prompt("A"))
var x = 0 
for(i = 0 ; i < 1000; i++) {
    for(ii = 0 ; ii < a ; ii++) {
        N.push(x)
        x++   
    }
    var x = 0 
}
var i = 0
for(i = 0 ; i < 1000; i++) {
        console.log("N[" + i + "] = " + N[i])
}
